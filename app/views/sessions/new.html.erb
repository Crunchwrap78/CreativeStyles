<div class="container">
<% provide(:title, "Sign in") %>


<div class="sign-in row">
<h4>Sign in</h4><br><br>
  <div class="col s12">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Sign in", class: "waves-effect waves-light btn-large" %>
    <% end %>

      <p>New user? <%= link_to "Sign up now!", signup_path %></p>
  </div>
</div>
